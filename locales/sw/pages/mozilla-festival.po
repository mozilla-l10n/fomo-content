msgstr ""

msgctxt "body.4ef1c6a8-1f8d-463b-b6e0-d014c2474355.text"
msgid "100s of sessions"
msgstr ""

msgctxt "body.4ef1c6a8-1f8d-463b-b6e0-d014c2474355.text"
msgid "Immersive sessions that teach privacy best practices, develop solutions to online misinformation and harassment, build open-source tools, support Trustworthy AI innovations, and more"
msgstr ""

msgctxt "body.48a61463-6791-405e-b6f5-dd1c69768a96.altText"
msgid "A photograph of the MozFest Opening Circle, with a large group of participants all with their hands raised in the air cheering on a video conferencing platform."
msgstr ""

msgctxt "body.48a61463-6791-405e-b6f5-dd1c69768a96.text"
msgid "1,000s of participants"
msgstr ""

msgctxt "body.48a61463-6791-405e-b6f5-dd1c69768a96.text"
msgid "Artists, activists, technologists, designers, students, and journalists from across the world attend MozFest each year."
msgstr ""

