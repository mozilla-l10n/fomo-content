msgctxt "body.203bf178-a871-406b-9d37-71667c5e1da1.cards.eccb6137-7ede-45ee-aca6-798226ae9fe3.highlighted_metadata"
msgid "Awe as an Antidote to the Grid"
msgstr "El asombro como antídoto para las mentes cuadriculadas"

msgctxt "body.203bf178-a871-406b-9d37-71667c5e1da1.cards.eccb6137-7ede-45ee-aca6-798226ae9fe3.metadata"
msgid "Plenary"
