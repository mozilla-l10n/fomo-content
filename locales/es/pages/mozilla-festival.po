msgid "Jason Silva on stage at Mozilla Festival"
msgstr "Jason Silva en el escenario del Festival Mozilla"

msgctxt "body.9a75260c-c7a8-4f33-913f-4217e3e92728.cards.ffee5693-79dc-4f90-8fe9-9ae27a52ed13.title"
msgid "Jason Silva"
msgstr "Jason Silva"

msgctxt "body.9a75260c-c7a8-4f33-913f-4217e3e92728.cards.ffee5693-79dc-4f90-8fe9-9ae27a52ed13.body"
msgid "Jason Silva invites us to step outside the algorithm and unmap the self, reimagining unlearning as liberation and wonder as a way forward."
msgstr "Jason Silva nos invita a salir del algoritmo y desvincularnos del yo, reimaginando el desaprendizaje como una liberación y el asombro como un camino a seguir."

msgctxt "body.9a75260c-c7a8-4f33-913f-4217e3e92728.cards.ffee5693-79dc-4f90-8fe9-9ae27a52ed13.link.57cef3e4-9e46-4044-bd24-dea2c9284f70.label"
