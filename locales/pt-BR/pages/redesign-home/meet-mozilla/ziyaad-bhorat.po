msgctxt "hero_description"
msgid "Vice President, Imagination and Strategic Growth"
msgstr "Vice-presidente de Imaginação e Crescimento Estratégico"

msgctxt "body.0db72797-f4c1-4083-8712-297b0b7c6771.left_column.9b527d32-a3c8-47e9-adc6-1cbe8ebc9fbc.image.alt_text"
