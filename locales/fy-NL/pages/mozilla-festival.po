msgstr "Bliuw by mei MozFest On-Demand!"

msgctxt "body.65f8eed7-9212-4e63-88c6-a2e00128357c.current_events.55a11513-b17c-4610-8c50-e447e57c920b.title"
msgid "Opening Circle"
msgstr "Iepeningssirkel"

msgctxt "body.65f8eed7-9212-4e63-88c6-a2e00128357c.current_events.55a11513-b17c-4610-8c50-e447e57c920b.subheading_link.d08c6e9c-98e7-48d0-ba22-86a21decf064.label"
msgid "Community Plenary"
msgstr "Plenêre gearkomst fan de mienskip"

msgctxt "body.65f8eed7-9212-4e63-88c6-a2e00128357c.current_events.55a11513-b17c-4610-8c50-e447e57c920b.body"
msgid "Rewatch the opening of our 5 day festival with Mozilla, hear from the community on why MozFest is so important as a place to connect and collaborate."
msgstr "Sjoch de iepening fan ús 5-daagse festival werom mei Mozilla, hear fan de mienskip wêrom MozFest sa wichtich is as in plak om kontakt te meitsjen en gear te wurkjen."

msgctxt "body.65f8eed7-9212-4e63-88c6-a2e00128357c.current_events.55a11513-b17c-4610-8c50-e447e57c920b.buttons.c656d9e7-43ec-4d88-aa27-b39d99406c0c.label"
