msgid "Mark yourself as attending on our LinkedIn event"
msgstr "Markearje josels as oanwêzich op ús LinkedIn-evenemint"

msgctxt "body.2c5463ba-5f3c-4bba-a961-745ac8a19a08.grid_items.2aa757c0-9f08-45a7-981c-febe0715bfe6.alt_text"
msgid "mastodon logo"
msgstr "Mastodon-logo"

msgctxt "body.2c5463ba-5f3c-4bba-a961-745ac8a19a08.grid_items.2aa757c0-9f08-45a7-981c-febe0715bfe6.caption"
