msgctxt "name"
msgid "Early Bird Festival Pass"
msgstr "Early Bird-festivalpas"

msgctxt "cost"
msgid "K15250"
msgstr "K15250"

msgctxt "group"
msgid "Early Bird Festival Pass"
msgstr "Early Bird-festivalpas"

msgctxt "description"
msgid "Festival Passes grant you full access to MozFest House Amsterdam from 11-13 June. Save €50 by purchasing the Early Bird ticket."
msgstr "Festivalpassen geven u van 11-13 juni volledige toegang tot MozFest House Amsterdam. Bespaar €50 door het Early Bird-ticket te kopen."

msgctxt "description"
msgid "Get access to engaging sessions, workshops, and our flagship Dialogues and Debates panels."
msgstr "Krijg toegang tot boeiende sessies, workshops en onze belangrijkste panels voor Dialogen en Debatten."

msgctxt "description"
msgid "Ensure your spot at our Songs of the Living experience."
msgstr "Verzeker u van een plek bij onze Songs of the Living-ervaring."

msgctxt "description"
msgid "Explore a variety of art installations and exhibits."
msgstr "Verken een verscheidenheid aan kunstinstallaties en tentoonstellingen."

msgctxt "description"
msgid "Attend our vibrant welcome party on 11 June."
msgstr "Woon ons levendige welkomstfeest bij op 11 juni."

msgctxt "description"
msgid "The price includes Dutch VAT at 21%."
msgstr "De prijs is inclusief 21% btw."

msgctxt "sticker_text"
msgid "Save 20%"
msgstr "Bespaar 20%"

msgctxt "button_text"
msgid "Get Your Ticket"
msgstr "Koop uw ticket"

#~ msgctxt "cost"
#~ msgid "€150"
#~ msgstr "€150"
