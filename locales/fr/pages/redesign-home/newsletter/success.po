msgctxt "title"
msgid "Newsletter Signup Success"
msgstr "Inscription à notre newsletter réussie"

msgctxt "body.ba09f0fd-0683-4733-aebf-8b2f252859be"
msgid "Your subscription is confirmed."
msgstr "Votre inscription est confirmée."

msgctxt "body.ba09f0fd-0683-4733-aebf-8b2f252859be"
msgid "From now on, you’ll receive updates directly from Mozilla Foundation."
msgstr "Désormais, vous recevrez directement l’actualité de la Fondation Mozilla."

msgctxt "body.ba09f0fd-0683-4733-aebf-8b2f252859be"
msgid "<b>Please add</b> <a id=\"a1\"><b>hello@eml.mozillafoundation.org</b></a><b> to your address book to ensure our emails reach your inbox.</b>"
msgstr "<b>Veuillez ajouter</b> <a id=\"a1\"><b>hello@eml.mozillafoundation.org</b></a><b> à votre carnet d’adresses pour vous assurer que nos e-mails arrivent dans votre boîte de réception.</b>"
