msgstr "wednesday-21st-june-day-ticket-copy"

msgctxt "body.e4d7d870-b528-44d3-a1b1-74eddeae0e47"
msgid "Festival Day Passes - €25"
msgstr "Pass 1 jour de festival - 25 €"

msgctxt "body.e4d7d870-b528-44d3-a1b1-74eddeae0e47"
msgid "Access all sessions and social spaces at MozFest House: Amsterdam on Tuesday, 20th June 2023 or Wednesday, 21st June 2023."
msgstr "Accédez à toutes les sessions et aux espaces sociaux du MozFest House: Amsterdam le mardi 20 juin 2023 ou le mercredi 21 juin 2023."

msgctxt "body.f3f28cb0-1a1a-47b1-9c5c-d753d1f4b657.button_label"
