msgstr "anciens membres"

msgctxt "body.a6c670a3-ffcd-4be0-a522-8bdaac6797fb.cards.c72fc379-aad3-4288-a5e5-6a01d2e9f8c5.title"
msgid "Alumni Connection Grants"
msgstr ""

msgctxt "body.a6c670a3-ffcd-4be0-a522-8bdaac6797fb.cards.c72fc379-aad3-4288-a5e5-6a01d2e9f8c5.body"
msgid ""
"Mozilla seeks to explore how alumni can nurture relationships with each other, connect as a network, explore collaboration and share their impactful work with the world. The application period has "
"ended."
msgstr ""

