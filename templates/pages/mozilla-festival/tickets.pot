msgid "Buy Full Festival Pass"
msgstr ""

msgctxt "body.ca645542-222c-462e-a6de-cd4f16fcb45b.releases"
msgid "wednesday-21st-june-day-ticket-copy"
msgstr ""

msgctxt "body.e4d7d870-b528-44d3-a1b1-74eddeae0e47"
msgid "Festival Day Passes - €25"
msgstr ""

msgctxt "body.e4d7d870-b528-44d3-a1b1-74eddeae0e47"
msgid "Access all sessions and social spaces at MozFest House: Amsterdam on Tuesday, 20th June 2023 or Wednesday, 21st June 2023."
msgstr ""

